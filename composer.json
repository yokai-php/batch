{
  "name": "yokai/batch",
  "description": "Batch architecture library inspired by Akeneo Batch",
  "license": "MIT",
  "authors": [
    {
      "name": "Yann Eugon√©",
      "email": "eugone.yann@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "psr/container": "^1.0",
    "psr/event-dispatcher": "^1.0"
  },
  "require-dev": {
    "ext-json": "*",
    "ext-zip": "*",
    "box/spout": "^3.0",
    "doctrine/orm": "^2.7",
    "phpunit/phpunit": "^7.5",
    "squizlabs/php_codesniffer": "^3.5",
    "symfony/console": "^4.4",
    "symfony/filesystem": "^4.4",
    "symfony/framework-bundle": "^4.4",
    "symfony/process": "^4.4",
    "symfony/serializer": "^4.4",
    "symfony/validator": "^4.4"
  },
  "suggest": {
    "box/spout": "Read/Write from/to CSV/ODS/XLSX (^3.0)",
    "doctrine/orm": "Read/Write from/to Doctrine ORM entities (^2.7)",
    "symfony/console": "Add command to trigger jobs and async job launcher via command (^4.4)",
    "symfony/filesystem": "Store job executions to filesystem (^4.4)",
    "symfony/framework-bundle": "Integrate to Symfony framework via a bundle (^4.4)",
    "symfony/process": "Used to determine PHP bin location when using command job launcher (^4.4)",
    "symfony/serializer": "Process items using (de)normalization, serialize job execution for certain storages (^4.4)",
    "symfony/validator": "Skip invalid items during process (^4.4)"
  },
  "conflict": {
    "box/spout": "<3.0",
    "doctrine/orm": "<2.7",
    "symfony/console": "<4.4",
    "symfony/filesystem": "<4.4",
    "symfony/framework-bundle": "<4.4",
    "symfony/process": "<4.4",
    "symfony/serializer": "<4.4",
    "symfony/validator": "<4.4"
  },
  "autoload": {
    "psr-4": {
      "Yokai\\Batch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yokai\\Batch\\Tests\\Integration\\": "tests/integration/",
      "Yokai\\Batch\\Tests\\Unit\\": "tests/unit/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.x-dev"
    }
  }
}
